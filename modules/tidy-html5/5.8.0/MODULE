"""Tidy Html C++ Bazel Module definition."""
module(
    name = "tidy-html5",
    version = "5.8.0",
)

bazel_dep(name = "rules_cc", version = "0.0.5")
bazel_dep(name = "platforms", version = "0.0.6")
